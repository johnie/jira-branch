{
  "name": "jira-branch",
  "version": "1.0.1",
  "description": "Retrieve JIRA information from current branch",
  "license": "MIT",
  "repository": "johnie/jira-branch",
  "author": {
    "name": "Johnie Hjelm",
    "email": "johnie@hjelm.im",
    "url": "https://johnie.se"
  },
  "engines": {
    "node": ">=12"
  },
  "type": "module",
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "jira",
    "branch",
    "branch name"
